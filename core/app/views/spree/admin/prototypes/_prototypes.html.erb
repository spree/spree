<table class="index">
  <thead>
    <tr data-hook="avilable_header">
      <th><%= t(:name) %></th>
      <th class="actions"></th>
    </tr>
  </thead>
  <tbody>
    <% @prototypes.each do |prototype| %>
      <tr id="row_<%= prototype.id %>" data-hook="available_row">
        <td><%= prototype.name %></td>
        <td class="actions">
          <%= link_to_with_icon 'icon-ok', t(:select), select_admin_prototype_url(prototype), :class => 'ajax iconlink select_properties_from_prototype button', :no_text => true %>
        </td>
      </tr>
    <% end %>
    <% if @prototypes.empty? %>
     <tr data-hook="available_none"><td colspan="2"><% t(:none) %>.</td></tr>
    <% end %>
  </tbody>
</table>
