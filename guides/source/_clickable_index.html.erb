<dl class="L">
  <% lgroup.each do |group, guides| %>
    <dt><%= group %></dt>
    <% guides.each do |guide| %>
      <dd><%= guide_link(guide) %></dd>
    <% end %>
  <% end %>
</dl>
<dl class="R">
  <% rgroup.each do |group, guides| %>
    <dt><%= group %></dt>
    <% guides.each do |guide| %>
      <dd><%= guide_link(guide) %></dd>
    <% end %>
  <% end %>
</dl>

