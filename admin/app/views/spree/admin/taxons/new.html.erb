<% content_for :page_title do %>
  <%= page_header_back_button spree.admin_taxonomy_path(@taxonomy) %>
  <%= Spree.t(:new_taxon) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @taxon } %>

<%= form_for @taxon, as: :taxon, url: spree.admin_taxonomy_taxons_path do |f| %>
  <%= render 'form', f: f %>

  <%= render 'spree/admin/shared/new_resource_links' %>
<% end %>
