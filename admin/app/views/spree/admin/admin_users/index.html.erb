<%= render 'spree/admin/shared/team_nav' %>

<% content_for :page_actions do %>
  <%= render_admin_partials(:admin_users_actions_partials) %>
<% end %>

<%= render_admin_partials(:admin_users_header_partials) %>

<div class="card-lg">
  <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <th scope="col" class="rounded-lg-top-left"><%= Spree.t(:user) %></th>
          <th scope="col"><%= Spree.t(:roles) %></th>
          <th scope="col" class="rounded-lg-top-right"><%= sort_link @search, :created_at, Spree.t(:created_at) %></th>
        </tr>
      </thead>
      <tbody>
        <%= render partial: 'spree/admin/admin_users/admin_user', collection: @collection %>
      </tbody>
    </table>
  </div>
</div>