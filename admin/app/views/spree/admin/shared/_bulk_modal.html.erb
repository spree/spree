<dialog class="dialog overflow-visible" data-bulk-dialog-target="dialog" id="bulk-dialog">
  <%= turbo_frame_tag :bulk_dialog do %>
    <%= dialog_header(Spree.t(:loading), 'bulk-dialog') %>
    <div class="dialog-body text-center overflow-visible">
      <%= render 'spree/admin/shared/spinner' %>
    </div>
    <div class="dialog-footer">
      <%= dialog_discard_button('bulk-dialog') %>
      <%= turbo_save_button_tag %>
    </div>
  <% end %>
</dialog>
