<%= turbo_frame_tag :dialog_modal do %>
  <%= form_for @import, url: spree.admin_imports_path do |f| %>
    <%= f.hidden_field :type %>

    <div class="modal-content">
      
      <div class="modal-body">
        <%= render 'spree/admin/shared/error_messages', target: @import %>
        <div class="modal-body pb-0" data-turbo-permanent id="DONTKNOW" %>
          <%= f.file_field :attachment, required: true, direct_upload: true, class: "form-control w-100" %>
        </div>
      </div>
      <div class="modal-footer">
        <%= modal_discard_button %>
        <%= turbo_save_button_tag Spree.t(:import), class: 'btn btn-primary' %>
      </div>
    </div>
  <% end %>
<% end %>