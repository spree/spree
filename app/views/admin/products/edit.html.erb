<%= render :partial => 'shared/product_sub_menu' %>
<h1><%= t("Editing Product")%></h1>
<%= render :partial => 'shared/product_tabs', :locals => {:current => "Product Details"} %>
<% form_for(:product, :url => object_url, :html => { :method => :put, :multipart => true }) do |f| %>
