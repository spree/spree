{
  "name": "@spree/sdk",
  "version": "0.6.0",
  "description": "Official TypeScript SDK for Spree Commerce API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      }
    },
    "./zod": {
      "import": {
        "types": "./dist/zod/index.d.ts",
        "default": "./dist/zod/index.js"
      },
      "require": {
        "types": "./dist/zod/index.d.cts",
        "default": "./dist/zod/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "generate:zod": "tsx scripts/generate-zod.ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "console": "tsx console.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "keywords": [
    "spree",
    "commerce",
    "ecommerce",
    "api",
    "sdk",
    "typescript",
    "headless"
  ],
  "author": "Vendo Connect Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spree/spree.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://spreecommerce.org",
  "bugs": {
    "url": "https://github.com/spree/spree/issues"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "msw": "^2.12.9",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0",
    "zod": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
